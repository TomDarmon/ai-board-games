{
	"$schema": "https://unpkg.com/knip@latest/schema.json",
	"entry": [
		"src/app/**/*.{ts,tsx}",
		"src/components/**/*.{ts,tsx}",
		"src/providers/**/*.{ts,tsx}",
		"src/server/**/*.{ts,tsx}",
		"src/hooks/**/*.{ts,tsx}",
		"src/lib/**/*.{ts,tsx}",
		"src/trpc/**/*.{ts,tsx}",
		"src/env.js"
	],
	"project": ["src/**/*.{ts,tsx,js}"],
	"ignore": ["src/server/games/@types.ts"],
	"ignoreDependencies": ["minimatch", "tailwindcss", "tw-animate-css", "crypto"]
}
